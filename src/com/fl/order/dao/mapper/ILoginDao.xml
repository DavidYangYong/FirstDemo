<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- @author zzp @version 2016-2-23 12:51:06 -->
<mapper namespace="com.fl.order.dao.mapper.ILoginDao">
	<resultMap type="Login" id="LoginResult">
		<result property="id" column="ID" />
		<result property="lastName" column="LAST_NAME" />
	</resultMap>
	<insert id="saveLogin" parameterType="Login">
		<selectKey resultType="java.lang.Long" order="BEFORE"
			keyProperty="id"> SELECT T_LOGIN_SEQ.nextval AS id FROM dual
		</selectKey>
		INSERT INTO T_LOGIN ( ID,LAST_NAME ) VALUES ( #{id,jdbcType=BIGINT},
		#{lastName,jdbcType=VARCHAR} )
	</insert>
</mapper>